<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<TableSelectDialog noDataText="Nenhum Grupo de mercadorias foi encontrado" title="Selecionar Grupo de mercadorias" search="handleValueHelpSearchGrupoMercadorias"
		liveChange="handleValueHelpSearchGrupoMercadorias" confirm="handleCloseGrupoMercadorias" multiSelect="false" growingThreshold="500" items="{GruposMercadorias}">
		<ColumnListItem>
			<cells>
				<Text text="{idGrupoMercadorias}"/>
				<Text text="{descricaoGrupoMercadorias}"/>
			</cells>
		</ColumnListItem>
		<columns>
			<Column width="12em">
				<header></header>
			</Column>
			<Column width="12em">
				<header></header>
			</Column>
		</columns>
	</TableSelectDialog>
</core:FragmentDefinition>